{
  "installCommand": "npm install --save-dev typescript@5.9.2 @types/react@19.1.15 @types/node@22.18.7 --legacy-peer-deps && npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 30
    }
  },
  "env": {
    "NPM_CONFIG_PACKAGE_LOCK": "true",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_OPTIONS": "--max-old-space-size=4096"
  }
}