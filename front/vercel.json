{
  "installCommand": "npm install typescript@5.3.3 @types/react@18.2.48 @types/node@20.11.5 @types/react-dom@18.2.18 --save-dev --legacy-peer-deps && npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 60
    }
  },
  "env": {
    "NPM_CONFIG_PACKAGE_LOCK": "true",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NEXT_FORCE_INSTALL_DEPS": "true",
    "NEXT_PRIVATE_SKIP_SETUP": "true"
  }
}